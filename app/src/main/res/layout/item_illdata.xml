<?xml version="1.0" encoding="utf-8"?>
<com.woshiku.jkshospticaldoctor.activity.view.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@id/item_square_layout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@id/item_square_layout_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/ic_launcher"
        android:scaleType="fitXY" />
    <ImageView
        android:id="@id/item_square_layout_dele"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@mipmap/compose_delete"
        android:layout_alignParentRight="true"
        />
</com.woshiku.jkshospticaldoctor.activity.view.SquareLayout>