<?xml version="1.0" encoding="utf-8"?>
<com.mixiaoxiao.overscroll.OverScrollScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E9F0F6">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:clickable="true"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_marginTop="@dimen/person_center_gap_one"
            app:cardBackgroundColor="@android:color/white">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@id/item_personal_one_icon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:background="@mipmap/ic_launcher"
                    android:layout_marginLeft="@dimen/person_center_gap_two" />
                <TextView
                    android:id="@id/item_personal_interval_name"
                    android:layout_toRightOf="@id/item_personal_one_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="王医生"
                    android:layout_marginLeft="10dp"
                    android:textSize="18sp"
                    android:textColor="#666666"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/arrow_forward"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    />
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@id/item_personal_interval_rece_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                android:layout_marginTop="@dimen/person_center_gap_one">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/person_center_item_height"
                    android:orientation="horizontal">
                    <View
                        android:layout_width="@dimen/person_center_icon"
                        android:layout_height="@dimen/person_center_icon"
                        android:background="@mipmap/ico_time"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/person_center_gap_three"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="接诊时间"
                        android:layout_gravity="center_vertical"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/person_center_item_height"
                    android:orientation="horizontal">
                    <View
                        android:layout_width="@dimen/person_center_icon"
                        android:layout_height="@dimen/person_center_icon"
                        android:background="@mipmap/ico_address"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/person_center_gap_three"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="地址管理"
                        android:layout_gravity="center_vertical"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
            <android.support.v7.widget.CardView
                android:id="@id/item_personal_interval"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/person_center_item_height"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="horizontal">
                        <View
                            android:layout_width="@dimen/person_center_icon"
                            android:layout_height="@dimen/person_center_icon"
                            android:background="@mipmap/ico_tow"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/person_center_gap_three"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/personal_txt"
                            android:text="诊断时间间隔"
                            android:layout_centerVertical="true"
                            />
                    </LinearLayout>
                    <TextView
                        android:id="@id/item_personal_interval_time_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="00:30"
                        android:layout_marginRight="20dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:textColor="#B7B7B7"
                        />
                </RelativeLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@id/item_personal_rece_history"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                android:layout_marginTop="@dimen/person_center_gap_one">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/person_center_item_height"
                    android:orientation="horizontal">
                    <View
                        android:layout_width="@dimen/person_center_icon"
                        android:layout_height="@dimen/person_center_icon"
                        android:background="@mipmap/ico_pie"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/person_center_gap_three"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="接诊历史"
                        android:layout_gravity="center_vertical"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
            <android.support.v7.widget.CardView
                android:id="@id/item_personal_check_receipe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/person_center_item_height"
                    android:orientation="horizontal">
                    <View
                        android:layout_width="@dimen/person_center_icon"
                        android:layout_height="@dimen/person_center_icon"
                        android:background="@mipmap/ico_pencil"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/person_center_gap_three"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="核实处方"
                        android:layout_gravity="center_vertical"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                android:layout_marginTop="@dimen/person_center_gap_one">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/person_center_item_height"
                        android:orientation="horizontal">
                        <View
                            android:layout_width="@dimen/person_center_icon"
                            android:layout_height="@dimen/person_center_icon"
                            android:background="@mipmap/ico_wired"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/person_center_gap_three"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/personal_txt"
                            android:text="清除缓存"
                            android:layout_gravity="center_vertical"
                            />
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/personal_txt"
                        android:text="0.0B"
                        android:layout_marginRight="20dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:textColor="#B7B7B7"
                        />
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
            <android.support.v7.widget.CardView
                android:id="@id/item_personal_exit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/person_center_item_height"
                        android:orientation="horizontal">
                        <View
                            android:layout_width="@dimen/person_center_icon"
                            android:layout_height="@dimen/person_center_icon"
                            android:background="@mipmap/ico_exit"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="@dimen/person_center_gap_three"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/personal_txt"
                            android:text="退出登录"
                            android:layout_gravity="center_vertical"
                            />
                    </LinearLayout>
                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e2e2e2"
                android:layout_marginLeft="@dimen/person_center_line_gap"
                android:layout_marginRight="@dimen/person_center_line_gap"
                />
        </LinearLayout>
    </LinearLayout>
</com.mixiaoxiao.overscroll.OverScrollScrollView>